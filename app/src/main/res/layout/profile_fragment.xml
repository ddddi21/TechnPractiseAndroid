    <data>

        <variable
            name="vm"
            type="com.example.technpractiseandroid.profile.ProfileVm" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
